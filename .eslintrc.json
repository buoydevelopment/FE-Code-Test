
{
  "env":
  { "node": true,
    "browser": false,
    "es6": true
  },
  "parser": "babel-eslint",
  "extends": ["airbnb", "plugin:prettier/recommended"],
  "rules": {
    "react/jsx-filename-extension": [ 1,
      {
        "extensions":[ ".js", ".jsx"]
      }
    ],
    "react/forbid-prop-types": 0
  },
  "plugins": [
    "prettier"
  ]
}
